                                                    <td>@data.GL_Code</td>
                                                    <td>@data.EInvoiceNo2</td>
                                                    <td>@data.EInvoiceDate2</td>
                                                    <td>@data.EWayBillNo2</td>
                                                    <td>@data.EWayDate2</td>
                                                    <td>@data.AdvanceReceiptNumber</td>
                                                    <td>@data.IsInvoiceAmended</td>
                                                    <td>@data.RemarksInvoiceAmended</td>
                                                   </tr>
                                                    }
                                                    }
                                                    <tbody>
                                                    </tbody>

                                                    <tfoot>
                                                        <tr>
                                                            <th>Document Type</th>
                                                            <th>Invoice No</th>
                                                            <th>E-Invoice No</th>
                                                            <th>Invoice Date</th>
                                                            <th>Shipping bill No</th>
                                                            <th>Shipping bill date</th>
                                                            <th>HSN Code</th>
                                                            <th>Quantity</th>
                                                            <th>UQC</th>
                                                            <th>Description</th>
                                                            <th>Party Name</th>
