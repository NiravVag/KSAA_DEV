@section Scripts{
    <!-- Bootstrap Core JavaScript -->
    <script src="~/vendors/bower_components/bootstrap-validator/dist/validator.js"></script>

    <script type="text/javascript">
        function GenLiabilityControlSheet() {
            if ($("#month").val() === '') {
                $.toast({
                    text: "Please Select Month",
                    position: 'top-right',
                    loaderBg: '#f8b32d',
                    icon: 'error',
                    hideAfter: 3500,
                    stack: 6
                });
                return;
            }
            if ($("#year").val() === '') {
                $.toast({
                    text: "Please Select Year",
                    position: 'top-right',
                    loaderBg: '#f8b32d',
                    icon: 'error',
                    hideAfter: 3500,
                    stack: 6
                });
                return;
            }
            var month_value = $('#month :selected').val();
            var year_value = $('#year :selected').val();

            if (($("#month").val() !== '') && ($("#year").val() !== '')) {
                $("#divLoader").show();
                $.ajax({
                    //url: '@Url.Action("GenLiabilityControlSheet", "OutputLiabilityLedger")',
                    url: `/OutputLiabilityLedger/GenLiabilityControlSheet?m=${month_value}&y=${year_value}`,
                    //contentType: 'text/html',
                    type: "POST",
                    //cache: false,
                    success: function (data) {
                        //window.location.reload();
                        $("#divLoader").hide();
                        console.log(data);
                        $.toast({
                            text: "Create Liability Control Sheet Success",
                            position: 'top-right',
                            loaderBg: '#f8b32d',
                            icon: 'success',
                            hideAfter: 3500,
                            stack: 6,
                        })
                        setTimeout(function () {
                            window.location.href = "/OutputLiabilityLedger/LiabilityControlSheet?m=" + month_value + "&y=" + year_value
                        }, 2000);
                    },
                    error: function (jqXHR, textStatus, error) {
                        //alert(data);
                        $("#divLoader").hide();
                        $.toast({
                            text: jqXHR.responseJSON.message,
                            position: 'top-right',
                            loaderBg: '#f8b32d',
                            icon: 'error',
                            hideAfter: 3500,
                            stack: 6
                        });
                    }
                });
            }
        }
        function AddSL() {
            if ($("#month").val() === '') {
                $.toast({
                    text: "Please Select Month",
                    position: 'top-right',
                    loaderBg: '#f8b32d',
                    icon: 'error',
                    hideAfter: 3500,
                    stack: 6
                });
                return;
            }
            if ($("#year").val() === '') {
                $.toast({
                    text: "Please Select Year",
                    position: 'top-right',
                    loaderBg: '#f8b32d',
                    icon: 'error',
                    hideAfter: 3500,
                    stack: 6
                });
                return;
            }
            var month_value = $('#month :selected').val();
            var year_value = $('#year :selected').val();

            if (($("#month").val() !== '') && ($("#year").val() !== '')) {
                $("#divLoader").show();
                $.ajax({
                    //url: '@Url.Action("AddToSupply", "GL_Income")',
                    url: `/OutputLiabilityLedger/AddToSupply?m=${month_value}&y=${year_value}`,
                    contentType: 'text/html',
                    type: "POST",
                    cache: false,
                    success: function (data) {
                        $("#divLoader").hide();
                        $.toast({
                            text: "Added in SL Success",
                            position: 'top-right',
                            loaderBg: '#f8b32d',
                            icon: 'success',
                            hideAfter: 3500,
                            stack: 6,
                        })
                        setTimeout(function () {
                            //window.location.reload();
                            window.location.href = "/OutputLiabilityLedger/LiabilityControlSheet?m=" + month_value + "&y=" + year_value
                        }, 2000);
                    },
                    error: function (jqXHR, textStatus, error) {
                        $("#divLoader").hide();
                        $.toast({
                            text: jqXHR.responseJSON.message,
                            position: 'top-right',
                            loaderBg: '#f8b32d',
                            icon: 'error',
                            hideAfter: 3500,
                            stack: 6
                        });
                    }
                });
            }
        }

    </script>

