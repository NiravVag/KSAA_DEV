<script type="text/javascript">
    //$("table tr").editable(OPTIONS);
    //alert("test");
    $(document).ready(function() {
        //alert("test 2");
        $("#divLoader").show();
        setTimeout(function() {
            $('#edit_datable_2').DataTable({
                "lengthChange": false,
                "order": [[0, "desc"]]
            });
            $("#divLoader").hide();
        }, 5000);
        $('#month').val(@ViewBag.Month)
        $('#year').val(@ViewBag.Year);
    });
    $(function() {
        //alert("1");
        $('table tr').editable({
            dblclick: false,
            edit: function(values) {
                $(".edit i", this)
                    .removeClass('fa-pencil')
                    .addClass('fa-save')
                    .attr('title', 'Save');
            },
            save: function(values) {
                $(".edit i", this)
                    .removeClass('fa-save')
                    .addClass('fa-pencil')
                    .attr('title', 'Edit');

                var id = $(this).data('id');
                //var Invoice_Number = $(this).data('invoice_Number');
                //var Invoice_Number_LL = $(this).data('invoice_Number_LL');

                //updateLiabilityRegisterData(id,Invoice_Number,Invoice_Number_LL,values);
                updateLiabilityRegisterData(id, values);
            },
            cancel: function(values) {
                $(".edit i", this)
                    .removeClass('fa-save')
                    .addClass('fa-pencil')
                    .attr('title', 'Edit');
            }
        });
    });

    function updateLiabilityRegisterData(id, values) {
        //alert('Save Data');
        if ($("#month").val() === '') {
            $.toast({
                text: "Please Select Month",
                position: 'top-right',
                loaderBg: '#f8b32d',
                icon: 'error',
                hideAfter: 3500,
                stack: 6
            });
            return;
        }
        if ($("#year").val() === '') {
            $.toast({
                text: "Please Select Year",
                position: 'top-right',
                loaderBg: '#f8b32d',
                icon: 'error',
                hideAfter: 3500,
                stack: 6
            });
            return;
        }
        var month_value = $('#month :selected').val();
        var year_value = $('#year :selected').val();

        var obj = {
            Id: id,
            //Invoice_Number: $("#invoice_Number").val(),
            Central_Tax: values.cgstsl,
            State_UT_Tax: values.sgstsl,
            Integrated_Tax: values.igstsl,
            Cess_Amount: values.cesssl,
            //Invoice_Number_LL: $("#invoice_Number_LL").val(),
            CGSTLL: values.cgstll,
            SGSTLL: values.sgstll,
            IGSTLL: values.igstll,
            CessLL: values.cessll,
            Month: month_value,
            Year: year_value
        };
        if (($("#Id").val() !== '') && ($("#Id").val() !== '-1') && ($("#Id").val() !== null)) {
            $("#divLoader").show();
            $.ajax({
                url: '/OutputLiabilityLedger/UpdateLiabilityControlDataById',
                method: 'POST',
                data: obj,
                success: function(data) {
                    $("#divLoader").hide();
                    $.toast({
                        text: "Data as per Updated Success",
                        position: 'top-right',
                        loaderBg: '#f8b32d',
                        icon: 'success',
                        hideAfter: 3500,
                        stack: 6,
                    })
                    setTimeout(function() {
                        window.location.reload();
                    }, 1500);
                },
                error: function(result) {
                    $("#divLoader").hide();
                    $.toast({
                        text: "Data as per Invoice Not Tag",
                        position: 'top-right',
                        loaderBg: '#f8b32d',
                        icon: 'error',
                        hideAfter: 3500,
                        stack: 6,
                    });
                }
            });
        }
    }
</script>

<script type="text/javascript">
    function GenLiabilityControlSheet() {
        if ($("#month").val() === '') {
            $.toast({
                text: "Please Select Month",
                position: 'top-right',
                loaderBg: '#f8b32d',
                icon: 'error',
                hideAfter: 3500,
                stack: 6
            });
            return;
        }
        if ($("#year").val() === '') {
            $.toast({
                text: "Please Select Year",
                position: 'top-right',
                loaderBg: '#f8b32d',
                icon: 'error',
                hideAfter: 3500,
                stack: 6
            });
            return;
        }
        var month_value = $('#month :selected').val();
        var year_value = $('#year :selected').val();

        if (($("#month").val() !== '') && ($("#year").val() !== '')) {
            $("#divLoader").show();
            $.ajax({
                //url: '@Url.Action("GenLiabilityControlSheet", "OutputLiabilityLedger")',
                url: `/OutputLiabilityLedger/GenLiabilityControlSheet?m=${month_value}&y=${year_value}`,
                //contentType: 'text/html',
                type: "POST",
                //cache: false,
                success: function(data) {
                    //window.location.reload();
                    $("#divLoader").hide();
                    console.log(data);
                    $.toast({
                        text: "Create Liability Control Sheet Success",
                        position: 'top-right',
                        loaderBg: '#f8b32d',
                        icon: 'success',
                        hideAfter: 3500,
                        stack: 6,
                    })
                    setTimeout(function() {
                        window.location.href = "/OutputLiabilityLedger/LiabilityControlSheet?m=" + month_value + "&y=" + year_value
                    }, 2000);
                },
                error: function(jqXHR, textStatus, error) {
                    //alert(data);
                    $("#divLoader").hide();
                    $.toast({
                        text: jqXHR.responseJSON.message,
                        position: 'top-right',
                        loaderBg: '#f8b32d',
                        icon: 'error',
                        hideAfter: 3500,
                        stack: 6
                    });
                }
            });
        }
    }
    function AddSL() {
        if ($("#month").val() === '') {
            $.toast({
                text: "Please Select Month",
                position: 'top-right',
                loaderBg: '#f8b32d',
                icon: 'error',
                hideAfter: 3500,
                stack: 6
            });
            return;
        }
        if ($("#year").val() === '') {
            $.toast({
                text: "Please Select Year",
                position: 'top-right',
                loaderBg: '#f8b32d',
                icon: 'error',
                hideAfter: 3500,
                stack: 6
            });
            return;
        }
        var month_value = $('#month :selected').val();
        var year_value = $('#year :selected').val();

        if (($("#month").val() !== '') && ($("#year").val() !== '')) {
            $("#divLoader").show();
            $.ajax({
                //url: '@Url.Action("AddToSupply", "GL_Income")',
                url: `/OutputLiabilityLedger/AddToSupply?m=${month_value}&y=${year_value}`,
                contentType: 'text/html',
                type: "POST",
                cache: false,
                success: function(data) {
                    $("#divLoader").hide();
                    $.toast({
                        text: "Added in SL Success",
                        position: 'top-right',
                        loaderBg: '#f8b32d',
                        icon: 'success',
                        hideAfter: 3500,
                        stack: 6,
                    })
                    setTimeout(function() {
                        //window.location.reload();
                        window.location.href = "/OutputLiabilityLedger/LiabilityControlSheet?m=" + month_value + "&y=" + year_value
                    }, 2000);
                },
                error: function(jqXHR, textStatus, error) {
                    $("#divLoader").hide();
                    $.toast({
                        text: jqXHR.responseJSON.message,
                        position: 'top-right',
                        loaderBg: '#f8b32d',
                        icon: 'error',
                        hideAfter: 3500,
                        stack: 6
                    });
                }
            });
        }
    }

</script>

