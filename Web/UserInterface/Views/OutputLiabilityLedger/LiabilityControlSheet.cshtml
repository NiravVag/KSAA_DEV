                            <table class="table mb-0">

                                <tr>
                                    <th class="col-md-2"></th>
                                    <th class="col-md-2"></th>
                                    <th class="col-md-4">Total SL</th>
                                    <td>@Model.Sum_CentralTaxSL</td>
                                    <td>@Model.Sum_StateUTTaxSL</td>
                                    <td>@Model.Sum_IntegratedTaxSL</td>
                                    <td>@Model.Sum_CessAmountSL</td>
                                </tr>
                                <tr>
                                    <th class="col-md-2"></th>
