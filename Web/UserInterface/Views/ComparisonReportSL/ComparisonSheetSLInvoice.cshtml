                        })
                        setTimeout(function() {
                            //getComparisonReport(month_value, year_value, action_value);
                            window.location.href = "/ComparisonReportSL/ComparisonSheetSLInvoice?m=" + month_value + "&y=" + year_value + "&a=" + action_value;
                        }, 2000);
                    },
                    error: function(jqXHR, textStatus, error) {
                        //alert(data);
