        function SLEInvoiceUpdateAction(action, Comparison_Report_E_Invoice_Portal_ID) {
            if ($("#year").val() === '') {
                $.toast({
                    text: "Please Select Year",
                    position: 'top-right',
                    loaderBg: '#f8b32d',
                    icon: 'error',
                    hideAfter: 3500,
                    stack: 6
                })
                setTimeout(function() {
                    window.location.reload();
                }, 1000);
            }

            var action = action.value;
            var id = Comparison_Report_E_Invoice_Portal_ID;
            var updateType = "SL_E_Invoice";
            var year_value = $('#year :selected').val();

            if ($("#year").val() !== '') {
                if (action == "Data as per E-Invoice") {
                    //var alertMsg = "Do you want to update the data as per E-Invoice";
                    //alert(alertMsg);
                    swal({
                        title: "Are you sure?",
                        text: "Do you want to update the data as per E-Invoice",
                        icon: "warning",
                        buttons: true,
                        dangerMode: true,
                        allowOutsideClick: !1
                    }).then(function(t) {
                        if (t) {
                            $.ajax({
                                type: "POST",
                                url: '@Url.Action("SLEInvoiceUpdateAction", "ComparisonReportSL")',
                                data: { action: action, id: id, updateType: updateType, year: year_value },
                                dataType: "json",
                                cache: false,
                                success: function(result) {
                                    if (result.statusCode) {
                                        $.toast({
                                            text: "Data as per E Invoice Updated",
                                            position: 'top-right',
                                            loaderBg: '#f8b32d',
                                            icon: 'success',
                                            hideAfter: 3500,
                                            stack: 6,
                                        })
                                        setTimeout(function() {
                                            window.location.reload();
                                        }, 1000);
                                    }
                                    else {
                                        $.toast({
                                            text: "Data as per E Invoice Not Updated",
                                            position: 'top-right',
                                            loaderBg: '#f8b32d',
                                            icon: 'error',
                                            hideAfter: 3500,
                                            stack: 6,
                                        });
                                    }
                                },
                                error: function(result) {
                                    $.toast({
                                        text: "Data as per E Invoice Not Updated",
                                        position: 'top-right',
                                        loaderBg: '#f8b32d',
                                        icon: 'error',
                                        hideAfter: 3500,
                                        stack: 6,
                                    });
                                },
                                complete: function() {
                                    addRemoveLoader(true);
                                },
                            });
                        }
                    });
                }
                else if (action == "Data as per Supply") {
                    //var alertMsg = "Do you want to update the data as per Supply";
                    //alert(alertMsg);
                    swal({
                        title: "Are you sure?",
                        text: "Do you want to update the data as per Supply",
                        icon: "warning",
                        buttons: true,
                        dangerMode: true,
                        allowOutsideClick: !1
                    }).then(function(t) {
                        if (t) {
                            $.ajax({
                                type: "POST",
                                url: '@Url.Action("SLEInvoiceUpdateAction", "ComparisonReportSL")',
                                data: { action: action, id: id, updateType: updateType, year: year_value },
                                dataType: "JSON",
                                cache: false,

                                success: function(result) {
                                    if (result.statusCode) {
                                        $.toast({
                                            text: "Data as per Supply Updated",
                                            position: 'top-right',
                                            loaderBg: '#f8b32d',
                                            icon: 'success',
                                            hideAfter: 3500,
                                            stack: 6,
                                        })
                                        setTimeout(function() {
                                            window.location.reload();
                                        }, 1000);
                                    }
                                    else {
                                        $.toast({
                                            text: "Data as per Supply Not Updated",
                                            position: 'top-right',
                                            loaderBg: '#f8b32d',
                                            icon: 'error',
                                            hideAfter: 3500,
                                            stack: 6,
                                        });
                                    }
                                },
                                error: function(result) {
                                    $.toast({
                                        text: "Data as per Supply Not Updated",
                                        position: 'top-right',
                                        loaderBg: '#f8b32d',
                                        icon: 'error',
                                        hideAfter: 3500,
                                        stack: 6,
                                    });
                                },
                                complete: function() {
                                    addRemoveLoader(true);
                                },
                            });
                        }
                    });
                }

                else {
                    swal({
                        title: "Are you sure?",
                        text: "Do you want to update the data as per No Action",
                        icon: "warning",
                        buttons: true,
                        dangerMode: true,
                        allowOutsideClick: !1
                    }).then(function(t) {
                        if (t) {
                            $.ajax({
                                type: "POST",
                                url: '@Url.Action("SLEInvoiceUpdateAction", "ComparisonReportSL")',
                                data: { action: action, id: id, updateType: updateType, year: year_value },
                                dataType: "JSON",
                                cache: false,
                                success: function(result) {
                                    if (result.statusCode) {
                                        $.toast({
                                            text: "Data as per No Action Updated",
                                            position: 'top-right',
                                            loaderBg: '#f8b32d',
                                            icon: 'success',
                                            hideAfter: 3500,
                                            stack: 6,
                                        })
                                        setTimeout(function() {
                                            window.location.reload();
                                        }, 1000);
                                    }
                                    else {
                                        $.toast({
                                            text: "Data as per No Action Not Updated",
                                            position: 'top-right',
                                            loaderBg: '#f8b32d',
                                            icon: 'error',
                                            hideAfter: 3500,
                                            stack: 6,
                                        });
                                    }
                                },
                                error: function(result) {
                                    $.toast({
                                        text: "Data as per No Action Not Updated",
                                        position: 'top-right',
                                        loaderBg: '#f8b32d',
                                        icon: 'error',
